{
  "name": "todo-cli",
  "description": "A simple todo cli tool.",
  "version": "1.0.0",
  "author": "Kelly J Andrews<@kellyjandrews>",
  "bin": {
    "todo": "./bin/run"
  },
  "bugs": "https://github.com/kellyjandrews/todo-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/fs-extra": "^9.0.8",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.55",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kellyjandrews/todo-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "todo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kellyjandrews/todo-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "tslib": "^1.14.1"
  }
}
